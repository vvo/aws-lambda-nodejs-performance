    "cdk": "cdk",
    "test": "jest"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.51.0",
    "aws-cdk": "1.51.0",
    "jest": "^25.5.0"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-nodejs": "^1.51.0",
